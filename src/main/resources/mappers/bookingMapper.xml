<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper>
  
  	<select id="selectBooking" resultType="com.doongie.test.ajax.model.Booking">
  	
  		SELECT 
  			`name`
  			, date`
  			, `day`
  			, `headCount`
  			, `phoneNumber`
  			, `state`
  		FROM
  			`booking`

  	</select>
  
  
  
  </mapper>