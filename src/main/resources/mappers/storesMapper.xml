<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.doongie.test.database.dao.StoresDAO">

  	<select id="selectStoresList" resultType="com.doongie.test.database.model.Stores">
  		SELECT 
  			`id`
  			, `name`
  			, `phoneNumber`
  			, `address`
  			, `businessNumber`
  			, `introduce`
  			, `createdAt`
  			, `updatedAt`
  		FROM 
  			`store`
  			
  	
  	</select>	


</mapper>